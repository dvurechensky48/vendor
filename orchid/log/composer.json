{
  "name": "orchid/log",
  "description": "Orchid CMS logging package",
  "keywords": ["framework", "laravel", "orchid", "log"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://theorchid.github.io/",
  "support": {
    "issues": "https://github.com/theOrchid/Platform/issues",
    "source": "https://github.com/theOrchid/Platform"
  },
  "authors": [
    {
      "name": "Alexandr Chernyaev",
      "email": "bliz48rus@gmail.com"
    },
    {
      "name": "David Mjomba",
      "email": "smodavprivate@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "files": [
      "src/Orchid/Log/Support/helpers.php"
    ],
    "psr-4": {
      "Orchid\\": "src/Orchid"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Orchid\\Log\\LogServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
